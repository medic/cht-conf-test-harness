{
  "name": "medic-conf-test-harness",
  "version": "0.2.0",
  "description": "Test Framework for Medic Projects",
  "main": "./src/harness.js",
  "scripts": {
    "test": "mocha test/*.spec.js --timeout 25000 --slow 500",
    "build": "npm run docs && ./build.sh",
    "docs": "node ./node_modules/jsdoc/jsdoc src/*.js src/**/*.js ./JSDOC.md",
    "explore": "node ./project-explorer/build-assets.js --path=./test/collateral && cd project-explorer && webpack && google-chrome ./project-explorer.html",
    "eslint": "node ./node_modules/eslint/bin/eslint.js .",
    "travis": "npm run build && npm run eslint && npm test"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "medic-nootils": "^4.0.1",
    "moment": "^2.24.0",
    "nools": "^0.4.4",
    "pojo2xml": "^1.7.2",
    "sinon": "^7.5.0",
    "puppeteer-chromium-resolver": "^1.0.12",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@medic/eslint-config": "^1.1.0",
    "chai": "^4.2.0",
    "chai-exclude": "^2.0.2",
    "enketo-core": "4.41.6",
    "eslint": "^6.8.0",
    "jsdoc": "^3.6.3",
    "medic": "git+https://github.com/medic/medic.git#3.6.x",
    "medic-enketo-xslt": "^1.5.0",
    "mocha": "^6.1.4",
    "openrosa-xpath-evaluator": "^1.5.1",
    "raw-loader": "^1.0.0",
    "rewire": "^4.0.1",
    "underscore": "^1.10.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
