      <input ref="/cash_add_member/member_details/member_phone">
        <label ref="jr:itext('/cash_add_member/member_details/member_phone:label')"/>
      </input>
      <select1 ref="/cash_add_member/member_details/handicapped">
        <label ref="jr:itext('/cash_add_member/member_details/handicapped:label')"/>
        <item>
          <label ref="jr:itext('/cash_add_member/member_details/handicapped/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/cash_add_member/member_details/handicapped/no:label')"/>
          <value>no</value>
        </item>
        <item>
          <label ref="jr:itext('/cash_add_member/member_details/handicapped/dont_know:label')"/>
