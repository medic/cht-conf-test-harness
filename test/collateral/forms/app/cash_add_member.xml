      <bind nodeset="/cash_add_member/member_details/member_age_years" type="int" relevant=" /cash_add_member/member_details/member_known_dob  ='no'" required="true()"/>
      <bind nodeset="/cash_add_member/member_details/member_phone" type="int" jr:constraintMsg="jr:itext('/cash_add_member/member_details/member_phone:jr:constraintMsg')" constraint="regex(., '^(\d{2}\s?){4}$')" jr:requiredMsg="jr:itext('/cash_add_member/member_details/member_phone:jr:requiredMsg')" relevant=" /cash_add_member/member_details/registration/registered  = 'no'" required="true()"/>
      <bind nodeset="/cash_add_member/member_details/handicapped" type="select1" jr:requiredMsg="jr:itext('/cash_add_member/member_details/handicapped:jr:requiredMsg')" required="true()"/>
      <bind nodeset="/cash_add_member/member_details/join_date" type="date" jr:constraintMsg="jr:itext('/cash_add_member/member_details/join_date:jr:constraintMsg')" constraint=".&lt;now()" jr:requiredMsg="jr:itext('/cash_add_member/member_details/join_date:jr:requiredMsg')" required="true()"/>
      <bind nodeset="/cash_add_member/member_details/membership" type="select1" jr:requiredMsg="jr:itext('/cash_add_member/member_details/membership:jr:requiredMsg')" required="true()"/>
      <bind nodeset="/cash_add_member/member_details/member_has_account" type="select1" jr:requiredMsg="jr:itext('/cash_add_member/member_details/member_has_account:jr:requiredMsg')" required="true()"/>
      <bind nodeset="/cash_add_member/member_details/account" type="int" relevant=" /cash_add_member/member_details/member_has_account  = 'yes'" jr:requiredMsg="jr:itext('/cash_add_member/member_details/account:jr:requiredMsg')" required="true()"/>
      <bind nodeset="/cash_add_member/member_details/member_profile" relevant=" /cash_add_member/member_details/registration/registered  = 'no'"/>
      <bind nodeset="/cash_add_member/member_details/member_profile/name" type="string" calculate=" /cash_add_member/member_details/member_name "/>
      <bind nodeset="/cash_add_member/member_details/member_profile/sex" type="string" calculate=" /cash_add_member/member_details/member_sex "/>
      <bind nodeset="/cash_add_member/member_details/member_profile/date_of_birth" type="string" calculate=" /cash_add_member/member_details/member_dob "/>
      <bind nodeset="/cash_add_member/member_details/member_profile/phone_member" type="string" calculate=" /cash_add_member/member_details/member_phone "/>
      <bind nodeset="/cash_add_member/member_details/member_profile/type" type="string"/>
      <bind nodeset="/cash_add_member/member_details/member_profile/contact_type" type="string"/>
      <bind nodeset="/cash_add_member/member_details/member_profile/created_by_doc" type="string" calculate="."/>
      <bind nodeset="/cash_add_member/member_details/member_profile/parent/_id" type="string" calculate=" /cash_add_member/place_id "/>
      <bind nodeset="/cash_add_member/member_details/member_profile/parent/parent/_id" type="string" calculate=" /cash_add_member/site_id "/>
      <bind nodeset="/cash_add_member/member_details/member_profile/parent/parent/parent/_id" type="string" calculate=" /cash_add_member/supervision_id "/>
      <bind nodeset="/cash_add_member/member_details/member_profile/parent/parent/parent/parent/_id" type="string" calculate=" /cash_add_member/health_area_id "/>
      <bind nodeset="/cash_add_member/member_details/member_profile/parent/parent/parent/parent/parent/_id" type="string" calculate=" /cash_add_member/district_id "/>
      <bind nodeset="/cash_add_member/member_details/member_profile/parent/parent/parent/parent/parent/parent/_id" type="string" calculate=" /cash_add_member/region_id "/>
      <bind nodeset="/cash_add_member/member_details/member_profile/parent/parent/parent/parent/parent/parent/parent/_id" type="string" calculate=" /cash_add_member/central_id "/>
      <bind nodeset="/cash_add_member/patient_name" type="string" calculate=" /cash_add_member/member_details/member_name "/>
      <bind nodeset="/cash_add_member/member_age_in_years" type="string" calculate="floor( difference-in-months(  /cash_add_member/member_details/member_dob , now() ) div 12 )"/>
      <bind nodeset="/cash_add_member/patient_phone" type="string" calculate=" /cash_add_member/member_details/member_profile/phone_member "/>
      <bind nodeset="/cash_add_member/member_sex_fr" type="string" calculate="if( /cash_add_member/member_details/member_sex ='male', 'Homme', 'Femme')"/>
      <bind nodeset="/cash_add_member/member_sex_en" type="string" calculate="if( /cash_add_member/member_details/member_sex ='male', 'Male', 'Female')"/>
      <bind nodeset="/cash_add_member/summary/n_form_submission" readonly="true()" type="string"/>
      <bind nodeset="/cash_add_member/summary/cvss_details" readonly="true()" type="string"/>
      <bind nodeset="/cash_add_member/summary/n_village_name" readonly="true()" type="string"/>
      <bind nodeset="/cash_add_member/summary/n_member_age" readonly="true()" type="string" relevant=" /cash_add_member/member_details/member_known_dob  ='no'"/>
      <bind nodeset="/cash_add_member/summary/n_member_dob" readonly="true()" type="string" relevant=" /cash_add_member/member_details/member_known_dob  ='yes'"/>
      <bind nodeset="/cash_add_member/summary/n_manager_lead_name" readonly="true()" type="string"/>
      <bind nodeset="/cash_add_member/summary/n_treasure_name" readonly="true()" type="string"/>
      <bind nodeset="/cash_add_member/summary/n_number_of_members" readonly="true()" type="string"/>
      <bind nodeset="/cash_add_member/summary/n_meeting_period" readonly="true()" type="string" relevant=" /cash_add_member/member_details/member_has_account ='no'"/>
      <bind nodeset="/cash_add_member/summary/n_meeting_month" readonly="true()" type="string" relevant=" /cash_add_member/member_details/member_has_account ='yes'"/>
      <bind nodeset="/cash_add_member/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
    </model>
  </h:head>
  <h:body class="pages">
    <group appearance="field-list" ref="/cash_add_member/inputs">
