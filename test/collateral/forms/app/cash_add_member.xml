        <item>
          <label ref="jr:itext('/cash_add_member/member_details/membership/individual:label')"/>
          <value>individual</value>
        </item>
      </select1>
      <select1 ref="/cash_add_member/member_details/member_has_account">
        <label ref="jr:itext('/cash_add_member/member_details/member_has_account:label')"/>
        <item>
          <label ref="jr:itext('/cash_add_member/member_details/member_has_account/yes:label')"/>
          <value>yes</value>
        </item>
        <item>
          <label ref="jr:itext('/cash_add_member/member_details/member_has_account/no:label')"/>
          <value>no</value>
